	pages = {58--71},
	file = {Parmesan et al. - 2013 - Beyond climate change attribution in conservation .pdf:/Users/ric325/Zotero/storage/R2A22G5Z/Parmesan et al. - 2013 - Beyond climate change attribution in conservation .pdf:application/pdf}
}

