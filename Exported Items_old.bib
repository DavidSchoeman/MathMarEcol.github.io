	pages = {e16552},
	file = {Grantham et al. - 2011 - Accommodating Dynamic Oceanographic Processes and .pdf:/Users/ric325/Zotero/storage/CU2K84K5/Grantham et al. - 2011 - Accommodating Dynamic Oceanographic Processes and .pdf:application/pdf}
