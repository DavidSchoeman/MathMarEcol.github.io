	keywords = {biodiversity, climate change, climate velocity, climate-analog velocity, conservation},
	pages = {441--457},
	file = {Brito-Morales_etal_2018_Supplement_TREE_Velocity&Conservation.pdf:/Users/ric325/Zotero/storage/GNCZ3W93/Brito-Morales_etal_2018_Supplement_TREE_Velocity&Conservation.pdf:application/pdf;ScienceDirect Full Text PDF:/Users/ric325/Zotero/storage/QR56Q59A/Brito-Morales et al. - 2018 - Climate Velocity Can Inform Conservation in a Warm.pdf:application/pdf;ScienceDirect Snapshot:/Users/ric325/Zotero/storage/4KUIR23G/S0169534718300636.html:text/html}
